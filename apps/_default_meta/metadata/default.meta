# Application-level permissions
{%- for permission in conf.default_meta.permissions %}
#### Object: '{{permission.object}}'
[{{permission.object}}]
access = read : [ {{permission.read|default("*",true)|join(", ")}} ], write : [ {{permission.write|default("admin",true)|join(", ")}} ]
	{%- if permission.export_system %}
export = system{%-endif%}
{%endfor%}